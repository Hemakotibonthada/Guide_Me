{
  "name": "guide-me-travel",
  "integrations": {
    "capacitor": {}
  },
  "type": "angular",
  "angular": {
    "browserOptions": {
      "assets": [
        {
          "glob": "**/*",
          "input": "src/assets",
          "output": "assets"
        },
        {
          "glob": "**/*.svg",
          "input": "node_modules/ionicons/dist/ionicons/svg",
          "output": "./svg"
        },
        {
          "glob": "manifest.json",
          "input": "src",
          "output": "./"
        }
      ]
    }
  }
}
